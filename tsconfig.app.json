/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@services/*": ["src/app/services/*"],
      "@shared/*": ["src/app/shared/*"],
      "@models/*": ["src/app/models/*"],
      "@friends-page/*": ["src/app/components/friends-page/*"],
      "@games-page/*": ["src/app/components/games-page/*"],
      "@main-page/*": ["src/app/components/main-page/*"],
      "@library-page/*": ["src/app/components/library-page/*"],
      "@login-page/*": ["src/app/components/login-page/*"],
      "@profile-page/*": ["src/app/components/profile-page/*"],
      "@header/*": ["src/app/components/header/*"],
      "@notification/*": ["src/app/components/notification/*"],
    },
    "outDir": "./out-tsc/app",
    "types": [],
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
